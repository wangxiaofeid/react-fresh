{
  "name": "react-fresh",
  "version": "1.0.0",
  "description": "react最新技术栈201808",
  "main": "index.js",
  "scripts": {
    "dev": "webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "start": "npm run dev",
    "build": "export NODE_ENV=production && webpack --config build/webpack.prod.conf.js",
    "dll": "export NODE_ENV=production && node ./build/webpack.dll.conf.js",
    "dll:dev": "export NODE_ENV=development && node ./build/webpack.dll.conf.js",
    "build:watch": "export NODE_ENV=production && webpack --config build/webpack.prod.conf.js --watch --progress",
    "push": "git add . && git commit -m ${m} && git push",
    "create": "node build/create.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wangxiaofeid/react-fresh.git"
  },
  "keywords": [
    "webpack4",
    "react16",
    "react-router4"
  ],
  "author": "xiaofei.wang",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/wangxiaofeid/react-fresh/issues"
  },
  "homepage": "https://github.com/wangxiaofeid/react-fresh#readme",
  "dependencies": {
    "@xiaofei.wang/float.js": "^1.0.0",
    "antd": "^3.11.4",
    "axios": "^0.18.0",
    "lodash": "^4.17.11",
    "mobx": "^5.0.3",
    "mobx-react": "^5.2.3",
    "normalize.css": "^8.0.0",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-loadable": "^5.5.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.1",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "autoprefixer": "^9.1.5",
    "babel-loader": "^8.0.2",
    "babel-plugin-import": "^1.11.0",
    "babel-plugin-lodash": "^3.3.4",
    "clean-webpack-plugin": "^0.1.19",
    "colors": "^1.3.3",
    "css-loader": "^1.0.0",
    "cssnano": "^4.1.0",
    "gulp": "^4.0.0",
    "gulp-replace": "^1.0.0",
    "html-webpack-plugin": "^3.2.0",
    "less": "^3.8.1",
    "less-loader": "^4.1.0",
    "mini-css-extract-plugin": "^0.4.2",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "style-loader": "^0.21.0",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.16.1",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5",
    "webpack-merge": "^4.1.3"
  },
  "browserslist": [
    "cover 99.5% in CN"
  ],
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      [
        "@babel/plugin-proposal-decorators",
        {
          "legacy": true
        }
      ],
      "@babel/plugin-syntax-dynamic-import",
      "@babel/plugin-syntax-import-meta",
      [
        "@babel/plugin-proposal-class-properties",
        {
          "loose": true
        }
      ],
      "@babel/plugin-transform-runtime",
      [
        "import",
        {
          "libraryName": "antd",
          "style": true
        }
      ],
      "lodash"
    ]
  }
}
