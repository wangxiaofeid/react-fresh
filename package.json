{
    "name": "react-fresh",
    "version": "1.0.0",
    "description": "react最新技术栈201808",
    "main": "index.js",
    "scripts": {
        "dev": "webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
        "start": "npm run dev",
        "mock": "cross-env MOCK=true npm run dev",
        "build": "cross-env NODE_ENV=production webpack --config build/webpack.prod.conf.js",
        "dll": "cross-env NODE_ENV=production node ./build/webpack.dll.conf.js",
        "dll:dev": "cross-env NODE_ENV=development node ./build/webpack.dll.conf.js",
        "build:watch": "cross-env NODE_ENV=production webpack --config build/webpack.prod.conf.js --watch --progress",
        "push": "git add . && git commit -m ${m} && git push",
        "create": "node build/create.js"
    },
